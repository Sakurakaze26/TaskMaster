<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/statContainer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.child.CosmeticShop">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="1.0"
        app:layout_constraintHeight_percent="1.0"
        app:layout_constraintDimensionRatio="1:"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/defaultbg_placeholder" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.95"
        app:layout_constraintHeight_percent="0.95"
        app:layout_constraintDimensionRatio=""
        app:srcCompat="@drawable/rectangle_rounded" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/dropdownNavButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_shadowed_nostroke_states"
        android:fontFamily="@font/eb_garamond_semibold"
        android:stateListAnimator="@null"
        android:gravity="center"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintWidth_percent="0.125"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0.5"
    />

    <ImageView
        android:id="@+id/dropdownNavImg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="false"
        android:focusable="false"
        app:layout_constraintBottom_toBottomOf="@+id/dropdownNavButton"
        app:layout_constraintEnd_toEndOf="@+id/dropdownNavButton"
        app:layout_constraintStart_toStartOf="@+id/dropdownNavButton"
        app:layout_constraintTop_toTopOf="@+id/dropdownNavButton"
        app:srcCompat="@drawable/dropdown" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/currencyBox"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/button_shadowed_nostroke_states"
        android:fontFamily="@font/eb_garamond_semibold"
        android:stateListAnimator="@null"
        android:gravity="center"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintEnd_toEndOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toTopOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintWidth_percent="0.125"
        app:layout_constraintHeight_percent="0.06"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintVertical_bias="0.5"
        />

    <TextView
        android:id="@+id/currencyText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/eb_garamond_semibold"
        android:text="1 G"
        android:textAlignment="center"
        android:textColor="#3F3F3F"
        android:visibility="visible"
        app:autoSizeMaxTextSize="30sp"
        app:autoSizeMinTextSize="10sp"
        app:autoSizeStepGranularity="1sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="@+id/currencyBox"
        app:layout_constraintEnd_toEndOf="@+id/currencyBox"
        app:layout_constraintStart_toStartOf="@+id/currencyBox"
        app:layout_constraintTop_toTopOf="@+id/currencyBox"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintWidth_percent="0.25" />

    <ImageView
        android:id="@+id/cosmeticShopContainerTopTextContainer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintWidth_percent="0.45"
        app:layout_constraintHeight_percent="0.075"
        app:layout_constraintVertical_bias="0.0075"
        app:srcCompat="@drawable/rectangle_rounded" />

    <TextView
        android:id="@+id/cosmeticShopContainerTopTextContainerText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/eb_garamond_semibold"
        android:text="Cosmetics"
        android:textColor="#3F3F3F"
        android:visibility="visible"
        android:textAlignment="center"
        app:autoSizeMinTextSize="10sp"
        app:autoSizeMaxTextSize="30sp"
        app:autoSizeStepGranularity="0.1sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintStart_toStartOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintTop_toTopOf="@+id/cosmeticShopContainerTopTextContainer"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintWidth_percent="0.25"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="0.5" />

    <!--THIS IS WHERE EVERYTHING IN THE SHOP IS CONTAINED -->
    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintHeight_percent="0.75"
        app:layout_constraintWidth_percent="0.85"
        app:srcCompat="@drawable/rectangle_rounded" />


    <!--this is where the containers of all the items are -->
    <ImageView
        android:id="@+id/cosmeticShopContainerItemContainer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        android:focusable="true"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/imageView20"
        app:layout_constraintTop_toTopOf="@id/imageView20"
        app:layout_constraintHeight_percent="0.125"
        app:layout_constraintWidth_percent="0.8"
        android:layout_marginLeft="7.5dp"
        android:layout_marginTop="7.5dp"
        app:srcCompat="@drawable/rectangle_rounded" />

    <ImageView
        android:id="@+id/cosmeticShopContainerItemContainer2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        android:focusable="true"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/imageView20"
        app:layout_constraintTop_toBottomOf="@id/cosmeticShopContainerItemContainer"
        app:layout_constraintHeight_percent="0.125"
        app:layout_constraintWidth_percent="0.8"
        android:layout_marginLeft="7.5dp"
        android:layout_marginTop="7.5dp"
        app:srcCompat="@drawable/rectangle_rounded" />


    <!--this ones the picture containers -->
    <ImageView
        android:id="@+id/cosmeticShopContainerItemContainerItem"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/cosmeticShopContainerItemContainer"
        app:layout_constraintTop_toTopOf="@id/cosmeticShopContainerItemContainer"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintWidth_percent="0.2"
        android:layout_marginLeft="7.5dp"
        android:layout_marginTop="7.5dp"
        app:srcCompat="@drawable/rectangle_rounded" />

    <ImageView
        android:id="@+id/cosmeticShopContainerItemContainerItem2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:alpha="0.5"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="@id/cosmeticShopContainerItemContainer2"
        app:layout_constraintTop_toTopOf="@id/cosmeticShopContainerItemContainer2"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintWidth_percent="0.2"
        android:layout_marginLeft="7.5dp"
        android:layout_marginTop="7.5dp"
        app:srcCompat="@drawable/rectangle_rounded" />

    <!--and this is the pictures it contained -->
    <ImageView
        android:id="@+id/cosmeticShopContainerItemContainetItemPicture"
        android:visibility="visible"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="false"
        android:focusable="false"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerItemContainerItem"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerItemContainerItem"
        app:layout_constraintStart_toStartOf="@+id/cosmeticShopContainerItemContainerItem"
        app:layout_constraintTop_toTopOf="@+id/cosmeticShopContainerItemContainerItem"
        app:srcCompat="@drawable/placeholderavatar1" />

    <ImageView
        android:id="@+id/cosmeticShopContainerItemContainetItemPicture2"
        android:visibility="visible"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="false"
        android:focusable="false"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerItemContainerItem2"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerItemContainerItem2"
        app:layout_constraintStart_toStartOf="@+id/cosmeticShopContainerItemContainerItem2"
        app:layout_constraintTop_toTopOf="@+id/cosmeticShopContainerItemContainerItem2"
        app:srcCompat="@drawable/placeholderavatar2" />

    <!--and this is the text for the cosmetic name -->
    <TextView
        android:id="@+id/cosmeticShopContainerItemContainerItemPictureText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/eb_garamond_semibold"
        android:text="Silver Armor"
        android:textColor="#3F3F3F"
        android:visibility="visible"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="10sp"
        app:autoSizeMaxTextSize="25sp"
        app:autoSizeStepGranularity="1sp"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerItemContainer"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerItemContainer"
        app:layout_constraintStart_toEndOf="@id/cosmeticShopContainerItemContainerItem"
        app:layout_constraintTop_toTopOf="@id/cosmeticShopContainerItemContainerItem"
        app:layout_constraintHorizontal_bias="0.10"
        app:layout_constraintVertical_bias="0"
        />

    <TextView
        android:id="@+id/cosmeticShopContainerItemContainerItemPictureText2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/eb_garamond_semibold"
        android:text="Bronze Armor"
        android:textColor="#3F3F3F"
        android:visibility="visible"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="10sp"
        app:autoSizeMaxTextSize="25sp"
        app:autoSizeStepGranularity="1sp"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerItemContainer2"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerItemContainer2"
        app:layout_constraintStart_toEndOf="@id/cosmeticShopContainerItemContainerItem2"
        app:layout_constraintTop_toTopOf="@id/cosmeticShopContainerItemContainerItem2"
        app:layout_constraintHorizontal_bias="0.10"
        app:layout_constraintVertical_bias="0"
        />

    <!--and this is the price cost text -->
    <TextView
        android:id="@+id/cosmeticShopContainerItemContainetItemPictureTextCost"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/eb_garamond_semibold"
        android:text="2 G"
        android:textAlignment="center"
        android:textColor="#3F3F3F"
        android:visibility="visible"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="1sp"
        app:autoSizeMaxTextSize="30sp"
        app:autoSizeStepGranularity="1sp"
        app:layout_constraintHeight_percent="0.025"
        app:layout_constraintWidth_percent="0.05"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerItemContainer"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerItemContainer"
        app:layout_constraintStart_toStartOf="@id/cosmeticShopContainerItemContainerItemPictureText"
        app:layout_constraintTop_toBottomOf="@id/cosmeticShopContainerItemContainerItemPictureText"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        />

    <TextView
        android:id="@+id/cosmeticShopContainerItemContainetItemPictureTextCost2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/eb_garamond_semibold"
        android:text="1 G"
        android:textAlignment="center"
        android:textColor="#3F3F3F"
        android:visibility="visible"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="1sp"
        app:autoSizeMaxTextSize="30sp"
        app:autoSizeStepGranularity="1sp"
        app:layout_constraintHeight_percent="0.025"
        app:layout_constraintWidth_percent="0.05"
        app:layout_constraintBottom_toBottomOf="@+id/cosmeticShopContainerItemContainer2"
        app:layout_constraintEnd_toEndOf="@+id/cosmeticShopContainerItemContainer2"
        app:layout_constraintStart_toStartOf="@id/cosmeticShopContainerItemContainerItemPictureText2"
        app:layout_constraintTop_toBottomOf="@id/cosmeticShopContainerItemContainerItemPictureText2"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintVertical_bias="0"
        />

    <ImageView
        android:id="@+id/confirmationTextContainer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="false"
        android:focusable="false"
        android:gravity="center"
        android:alpha="0.5"
        android:visibility="visible"
        app:srcCompat="@drawable/rectangle_rounded"
        android:fontFamily="@font/eb_garamond_semibold"
        app:layout_constraintHeight_percent="0.15"
        app:layout_constraintWidth_percent="0.75"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6" />

    <TextView
        android:id="@+id/confirmationTextContainerText"
        android:visibility="visible"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:textAlignment="center"
        android:text="Are you sure you want to buy this item?"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="5sp"
        app:autoSizeMaxTextSize="35sp"
        app:autoSizeStepGranularity="1sp"
        android:textColor="#3F3F3F"
        android:fontFamily="@font/eb_garamond_semibold"
        app:layout_constraintHeight_percent="0.075"
        app:layout_constraintWidth_percent="0.75"
        app:layout_constraintEnd_toEndOf="@+id/confirmationTextContainer"
        app:layout_constraintStart_toStartOf="@+id/confirmationTextContainer"
        app:layout_constraintTop_toTopOf="@+id/confirmationTextContainer"
         />

    <ImageView
        android:id="@+id/confirmationTextContainerChoiceYes"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:alpha="0.5"
        android:visibility="visible"
        app:srcCompat="@drawable/rectangle_rounded"
        android:fontFamily="@font/eb_garamond_semibold"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintWidth_percent="0.2"
        app:layout_constraintStart_toStartOf="@+id/confirmationTextContainer"
        app:layout_constraintEnd_toEndOf="@+id/confirmationTextContainer"
        app:layout_constraintTop_toTopOf="@id/confirmationTextContainer"
        app:layout_constraintBottom_toBottomOf="@id/confirmationTextContainer"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/confirmationTextContainerTextChoiceYesText"
        android:visibility="visible"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        android:focusable="false"
        android:textAlignment="center"
        android:text="Yes"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="5sp"
        app:autoSizeMaxTextSize="30sp"
        app:autoSizeStepGranularity="1sp"
        android:textColor="#3F3F3F"
        android:fontFamily="@font/eb_garamond_semibold"
        app:layout_constraintEnd_toEndOf="@+id/confirmationTextContainerChoiceYes"
        app:layout_constraintStart_toStartOf="@+id/confirmationTextContainerChoiceYes"
        app:layout_constraintTop_toTopOf="@+id/confirmationTextContainerChoiceYes"
        app:layout_constraintBottom_toBottomOf="@id/confirmationTextContainerChoiceYes"
        />

    <ImageView
        android:id="@+id/confirmationTextContainerChoiceNo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center"
        android:alpha="0.5"
        android:visibility="visible"
        app:srcCompat="@drawable/rectangle_rounded"
        android:fontFamily="@font/eb_garamond_semibold"
        app:layout_constraintHeight_percent="0.05"
        app:layout_constraintWidth_percent="0.2"
        app:layout_constraintStart_toStartOf="@+id/confirmationTextContainer"
        app:layout_constraintEnd_toEndOf="@+id/confirmationTextContainer"
        app:layout_constraintTop_toTopOf="@id/confirmationTextContainer"
        app:layout_constraintBottom_toBottomOf="@id/confirmationTextContainer"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintVertical_bias="0.8" />

    <TextView
        android:id="@+id/confirmationTextContainerTextChoiceNoText2"
        android:visibility="visible"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clickable="true"
        android:focusable="false"
        android:textAlignment="center"
        android:text="No"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="5sp"
        app:autoSizeMaxTextSize="30sp"
        app:autoSizeStepGranularity="1sp"
        android:textColor="#3F3F3F"
        android:fontFamily="@font/eb_garamond_semibold"
        app:layout_constraintEnd_toEndOf="@+id/confirmationTextContainerChoiceNo"
        app:layout_constraintStart_toStartOf="@+id/confirmationTextContainerChoiceNo"
        app:layout_constraintTop_toTopOf="@+id/confirmationTextContainerChoiceNo"
        app:layout_constraintBottom_toBottomOf="@id/confirmationTextContainerChoiceNo"
        />



